    function clear() {
        $("#validschle").val('');
        $("#validschleForSave").val('');


        /*$("#SCHLE").prop("readonly", false);*/

    }


    function SchoolCodeAvailability() { //This function call on text change.

        var selectedID = $("#psebCode").val();
        console.log("psebCode change", selectedID);
        $.ajax({
            type: "POST",
            url: "/AssociateCommuni/CheckSchoolCode", // this for calling the web method function in cs code.
            data: '{schoolcode: "' + $("#psebCode").val() + '" }',// user name or email value
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: OnSuccess,
            failure: function (response) {
                // alert(response);
            }

        });
    }


    function OnSuccess(response) {
        console.log("response", response)
        var msg = $('#lblStatus');
        msg.text("");
        switch (response.oid) {
            case "0":
                clear();
                msg.css("slidedown", "slow");
                msg.css("color", "red");
                /*   alert("Invalid School Code");*/
                $("#lblStatus").text("Invalid School Code");
                msg.text("Invalid School Code");
                $("#schlname").css("display", "none");

                break;
            case "5":
                clear();

                msg.text("Invalid School Code");
                msg.css("slidedown", "slow");
                msg.css("color", "red");
                msg.text("School Code Already Exists");
                $("#schlname").css("display", "none");

                break;
            case "1":
                msg.css("slidedown", "slow");
                msg.css("color", "green");
                $("#schlname").css("display", "block");
                //  msg.text("Valid School Code");
                console.log(response);


                $("#validschle").val(response.sm.SCHLNME);
                $("#validschleForSave").val(response.sm.SCHLNME);


                /*$("#validschle").prop("readonly", true);*/
                msg.text("Valid School Code , " + response.sm.SCHL + "");


                break;
        }
    }



</script>



